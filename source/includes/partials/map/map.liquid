<div id="map" class="w-full h-96 mt-10">
    <script type="text/javascript">
        const coords = [55.943381, 43.118325]
        ymaps.ready(init); 
        function init(){
            var myMap = new ymaps.Map("map", { 
            center: coords, 
            zoom: 12
            }); 
        var marker = new ymaps.Placemark (coords); 
        myMap.geoObjects.add(marker); 
        }
    </script>
</div>